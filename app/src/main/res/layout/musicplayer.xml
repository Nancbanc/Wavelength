<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#14213D"
    android:backgroundTint="#14213D">

    <ImageView
        android:id="@+id/songplayer"
        android:layout_width="320dp"
        android:layout_height="320dp"
        android:layout_marginTop="144dp"
        android:src="@drawable/ellipse_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/wl"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/shuffle"
        android:layout_width="97dp"
        android:layout_height="71dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="200dp"
        android:backgroundTint="#00000000"
        android:contentDescription="shuffle playlist"
        android:src="@drawable/vector__5_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/replay"
        android:layout_width="83dp"
        android:layout_height="79dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="198dp"
        android:backgroundTint="#00000000"
        android:contentDescription="replay"
        android:src="@drawable/ph_repeat_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/plus"
        android:layout_width="42dp"
        android:layout_height="43dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="348dp"
        android:src="@drawable/vector__6_"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/equalizer"
        android:layout_width="73dp"
        android:layout_height="70dp"
        android:layout_marginEnd="220dp"
        android:layout_marginBottom="18dp"
        android:backgroundTint="#00000000"
        android:contentDescription="music equalizer"
        android:src="@drawable/music_equalizer__music_audio_note_wave_sound_equalizer_entertainment__1_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/musichome"
        android:layout_width="75dp"
        android:layout_height="70dp"
        android:layout_marginEnd="125dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#00000000"
        android:contentDescription="home"
        android:src="@drawable/vector__7_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/add"
        android:layout_width="72dp"
        android:layout_height="61dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="#00000000"
        android:contentDescription="plus looking sign"
        android:src="@drawable/vector__6_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/skip"
        android:layout_width="98dp"
        android:layout_height="76dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="112dp"
        android:backgroundTint="#00000000"
        android:contentDescription="skip"
        android:src="@drawable/group__2_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageButton
        android:id="@+id/playpause"
        android:layout_width="94dp"
        android:layout_height="78dp"
        android:layout_marginStart="156dp"
        android:layout_marginBottom="112dp"
        android:backgroundTint="#00000000"
        android:contentDescription="playpause"
        android:src="@drawable/vector__9_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/goback"
        android:layout_width="95dp"
        android:layout_height="84dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="108dp"
        android:backgroundTint="#00000000"
        android:contentDescription="goback"
        android:src="@drawable/vector__10_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/durration"
        android:layout_width="149dp"
        android:layout_height="430dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="41dp"
        android:src="@drawable/line_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/profileback"
        android:layout_width="76dp"
        android:layout_height="68dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/ellipse_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profiletext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="24dp"
        android:text="@string/Profilepic"
        android:textColor="#FCA311"
        app:layout_constraintEnd_toEndOf="@+id/profileback"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/playlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="25dp"
        android:backgroundTint="#00000000"
        android:contentDescription="playlist"
        android:src="@drawable/group__1_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>